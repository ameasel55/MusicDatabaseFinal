@model IEnumerable<Music>


<h2>Music</h2>

<div style="display: block">
    <a href="/Music/InsertMusic/" style="text-shadow:1px 1px 0 #444">Add a new entry!</a>
</div>

<table class="table">
    <tr>
        <th>ID</th>
        <th>Artist Name</th>
        <th>Album Name</th>
        <th>Year Released</th>
        <th>Genre</th>
        <th>My Rating</th>
    </tr>

    @foreach (var music in @Model)
    {
        <tr>
            <td><a href=/Music/ViewMusic/@music.ID>@music.ID</a></td>
            <td>@music.ArtistName</td>
            <td>@music.AlbumName</td>
            <td>@music.YearReleased</td>
            <td>@music.Genre</td>
            <td>@music.MyRating</td>
        </tr>
    }

</table>
